GET     /assets/*file               controllers.Assets.versioned(path="/public", file: Asset)

GET     /                           controllers.HomeController.index

GET     /submit                     controllers.CDController.showCDForm
POST    /submit                     controllers.CDController.submitCD

GET     /contact                    controllers.PeopleController.showPeopleForm
POST    /contact                    controllers.PeopleController.submitPeople

GET     /messages                   controllers.PeopleController.showMessages